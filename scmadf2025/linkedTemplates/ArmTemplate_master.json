{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "scmadf2025"
		},
		"Git_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Git_LS'"
		},
		"Git_LS2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Git_LS2'"
		},
		"adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls'"
		},
		"adls_ls2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls2'"
		},
		"sql_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_LS'"
		},
		"Git_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Satya42-cloud/Supply_Chain_Management/refs/heads/main/Static_Data/"
		},
		"Git_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "satyajeetswain168@gmail.com"
		},
		"Git_LS2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/Satya42-cloud/Supply_Chain_Management/refs/heads/main/Stream_Data/"
		},
		"Git_LS2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "satyajeetswain168@gmail.com"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://scmdataset2025.dfs.core.windows.net/"
		},
		"adls_ls2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://scmdataset2025.dfs.core.windows.net/"
		},
		"sql_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serverscm.database.windows.net"
		},
		"sql_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "scmdatabase"
		},
		"sql_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Satya"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "scmadf2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Git_LS_password": {
						"value": "[parameters('Git_LS_password')]"
					},
					"Git_LS2_password": {
						"value": "[parameters('Git_LS2_password')]"
					},
					"adls_ls_accountKey": {
						"value": "[parameters('adls_ls_accountKey')]"
					},
					"adls_ls2_accountKey": {
						"value": "[parameters('adls_ls2_accountKey')]"
					},
					"sql_LS_password": {
						"value": "[parameters('sql_LS_password')]"
					},
					"Git_LS_properties_typeProperties_url": {
						"value": "[parameters('Git_LS_properties_typeProperties_url')]"
					},
					"Git_LS_properties_typeProperties_userName": {
						"value": "[parameters('Git_LS_properties_typeProperties_userName')]"
					},
					"Git_LS2_properties_typeProperties_url": {
						"value": "[parameters('Git_LS2_properties_typeProperties_url')]"
					},
					"Git_LS2_properties_typeProperties_userName": {
						"value": "[parameters('Git_LS2_properties_typeProperties_userName')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"adls_ls2_properties_typeProperties_url": {
						"value": "[parameters('adls_ls2_properties_typeProperties_url')]"
					},
					"sql_LS_properties_typeProperties_server": {
						"value": "[parameters('sql_LS_properties_typeProperties_server')]"
					},
					"sql_LS_properties_typeProperties_database": {
						"value": "[parameters('sql_LS_properties_typeProperties_database')]"
					},
					"sql_LS_properties_typeProperties_userName": {
						"value": "[parameters('sql_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}